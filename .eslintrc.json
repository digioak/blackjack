{
    "plugins": ["jest"],
    "env": {
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base",
        "plugin:jest/recommended"
    ],
    "parser": "@babel/eslint-parser",
    "rules": {
        "no-console": "off",
        "import/prefer-default-export": "off",
        "import/extensions": "off",
        "no-param-reassign": "off",
        "no-continue": "off"
    }
}
